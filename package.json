{
  "name": "natours",
  "version": "1.0.1",
  "description": "Landing page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css -o css/style.prefix.css -u autoprefixer",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "live-server": "live-server"
  },
  "author": "Vick",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.4.2",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vvtret/Natours.git"
  },
  "bugs": {
    "url": "https://github.com/vvtret/Natours/issues"
  },
  "homepage": "https://github.com/vvtret/Natours#readme"
}
